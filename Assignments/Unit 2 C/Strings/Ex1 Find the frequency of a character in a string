#include"stdio.h"
#include "string.h"

int main (){
    char text[100], letter;
	int i,loc=0;
	printf ("Enter a string : ");
	fflush(stdout);fflush(stdin);
	gets(text);
	printf ("Enter  a character to find frequency :");
	fflush(stdout);fflush(stdin);
	scanf ("%c",&letter);
	for (i=0;i<strlen(text);i++){
		if(text[i]==letter){
			loc+=1 ;
		}

	}
	printf("frequency of %c = %d",letter,loc);
	return 0;
}

