#include"stdio.h"

void main (){
	int num,i,fact=1;
	printf("Enter an Integer : ");
	fflush (stdin);fflush (stdout);
	scanf ("%d" ,&num);
	if (num >0){
		for(i=1;i<=num;i++){
			fact=fact*i;
		}
		printf("factorial = %d",fact);
	}
	else {
		printf("Error !!! factorial of negative number doesn't exist");
	}

}
