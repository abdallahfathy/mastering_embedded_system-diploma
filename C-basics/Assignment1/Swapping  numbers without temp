#include"stdio.h"

void main (){
	float x,y,temp;
	printf("Enter value of a: ");
	fflush(stdin);fflush(stdout);
	scanf ("%f",&x);
	printf("Enter value of b: ");
	fflush(stdin);fflush(stdout);
	scanf ("%f",&y);
	x=y+x;
	y=x-y;
	x=x-y;
	printf("After swapping, value of a = %.2f",x);
	printf("\nAfter swapping, value of b = %.2f",y);

}

