#include"stdio.h"

void main (){
	char op ;
	int num1,num2 ;
	printf("Enter an operator : ");
	fflush (stdin);fflush (stdout);
	scanf ("%c" ,&op);
	fflush (stdin);fflush (stdout);
	scanf ("%d %d" ,&num1,&num2);
	fflush (stdin);fflush (stdout);
	switch(op){
	case  '+':{
		printf("%d + %d = %d ",num1,num2,num1+num2);
	}
	break;
	case  '-':{
		printf("%d - %d = %d ",num1,num2,num1-num2);
	}
	break;
	case '*' :{	
		printf("%d * %d = %d ",num1,num2,num1*num2);
	}
	break;
	case '/' :{
		printf ("%d / %d = %d ",num1,num2,num1/num2);
	}
	break;
	default:{
		printf("no right operands");
	}
	}

}

